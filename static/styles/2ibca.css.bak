/* Reset de Estilos */
* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

/* Estilos Globais */
body {
  font-family: Arial, sans-serif;
  font-size: 16px;
  background-color: #f4f4f4;
}

.container {
width: 90%; /* Em vez de fixar uma largura em pixels */
margin: 0 auto; /* Centraliza horizontalmente */
}

h1 {
font-size: 2rem; /* Escala com o tamanho base do texto */
}

/* Responsividade do carrossel */
.carousel-inner img {
max-width: 100%;
height: auto; /* Ajusta automaticamente a altura mantendo a proporção */
}

/* Responsividade das legendas do carrossel */
.carousel-caption {
background-color: rgba(255, 255, 255, 0.7); /* Fundo branco semitransparente */
padding: 10px 20px; /* Espaçamento interno */
border-radius: 5px; /* Bordas arredondadas */
color: #000; /* Cor do texto */
text-shadow: none; /* Remove sombra do texto */
}

/* Ajustes para telas menores */
@media (max-width: 768px) {
.carousel-caption {
    font-size: 0.9rem; /* Reduz o tamanho da fonte */
    padding: 8px 15px; /* Ajusta o espaçamento */
}
}

/* Ajustes para smartphones */
@media (max-width: 576px) {
.carousel-caption {
    font-size: 0.8rem; /* Tamanho da fonte menor ainda */
    padding: 5px 10px; /* Espaçamento mais compacto */
}
}


/* Cabeçalho */
header {
  background: #007bff; /* Cor de fundo azul */
  color: #FFFFFF !important; /* Cor do texto branco */
  padding: 5px 15px; /* Espaçamento interno */
  display: flex;
  align-items: center;
  justify-content: space-between; /* Espaça o logotipo e o menu */
}

.sticky-top {
z-index: 1030; /* Garante que o menu fique na frente de tudo */
box-shadow: 0px 5px 8px rgba(0,0,0,0.2); /* Sombra leve no menu */
}

.logo {
  display: flex;
  align-items: flex-end;
  font-family: 'Arial Rounded MT Bold', Arial, sans-serif; /* Fonte mais arredondada */
  margin: 0;
  line-height: 1.2; /* Ajuste a altura da linha se necessário */
}

.logo img {
  height: 100px; /* Altura da logo (ajuste conforme necessário) */
  margin-right: 10px; /* Espaçamento entre a logo e o texto */
  transition: transform 0.5s ease; /* Animação suave ao interagir */
}

.logo a:hover img {
transform: scale(1.1); /* Aumenta ligeiramente a logo ao passar o mouse */
}

.menu{
font-weight: 500 !important;
padding: 10px 40px;
}


/* Alinhamento dos itens do menu à direita */
nav {
  display: flex;
  align-items: center;
}

nav ul {
  list-style: none;
  display: flex;
  gap: 20px; /* Espaço entre os itens do menu */
  margin: 0;
  padding: 0;
}

nav ul li {
  margin: 0;
}

nav a {
  text-decoration: none;
  color: #ffffff;
  font-weight: bold;
  font-size: 16px;
  padding: 8px 12px;
}

/* Estilos adicionais para o submenu */

/* Estilo para o item "Sobre" e seu dropdown */
.nav-item.dropdown:hover .dropdown-menu {
  display: block;
  /* Melhorar área de ativação do dropdown */
  margin-top: 0; /* tira o "espaço" entre o botão e o menu */
}

/* Personalização do estilo do dropdown */
.dropdown-menu {
  font-family: 'Arial Rounded MT Bold', Arial, sans-serif; /* Fonte mais arredondada */
  background-color: #ffffff; /* Cor de fundo do dropdown */
  border: 1px solid hsl(210, 50%, 90%); /* Define uma borda com cor usando HSL */
  border-radius: 8px; /* Define o arredondamento dos cantos */
  font-weight: 500 !important;
  box-shadow: 3px 5px 8px rgba(0, 0, 0, 0.2);
  /* Melhorar ainda mais o tempo de resposta */
  margin-top: 0px;
  transition: all 0.3s ease;
}

/* Melhorar aparência */
.dropdown-menu a {
padding: 10px 20px;

}

.dropdown-item {
  color: #10acec; /* Cor do texto no dropdown */
  font-weight: 700;
  font-style: inherit;
}

.dropdown-item:hover {
  background-color: #10acec; /* Cor de fundo ao passar o mouse no dropdown */
}

nav a:hover {
  background-color: #10acec;
}

.icon {
  width: 20px;
  height: 20px;
  margin-right: 5px;
  vertical-align: middle;
}

/* Estilos do Conteúdo da Página Aqui */

.menu {
  list-style: none;
  display: flex;
  
}

.menu-item {
  margin-right: 20px;
}

/* Adicione um contêiner para o conteúdo abaixo da navegação */
.content-container {
  max-width: 500px; /* Largura máxima do conteúdo */
  margin: 10px auto; /* Centraliza o contêiner */
}

/* Estilos adicionais para o conteúdo */

