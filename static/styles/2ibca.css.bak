/* Estilos para cards de pessoas */
.pessoa-card-section {
background-color: #007bff;
padding: 40px 0;
}

.titulo-pessoa {
color: white;
text-align: center;
margin-bottom: 30px;
font-size: 2.5rem;
font-weight: 700;
}

.descricao-pessoa {
color: white;
text-align: center;
max-width: 800px;
margin: 0 auto 40px;
font-size: 1.1rem;
line-height: 1.6;
}

.pessoas-wrapper {
display: flex;
justify-content: center;
gap: 30px;
flex-wrap: wrap;
width: 100%;
max-width: 1200px;
margin: 0 auto;
}

.pessoa-card {
background: white;
border-radius: 10px;
padding: 20px;
width: 280px; /* Largura fixa para os cards */
text-align: center;
box-shadow: 0 4px 8px rgba(0,0,0,0.1);
transition: transform 0.3s ease, box-shadow 0.3s ease;
margin-bottom: 20px;
}

.pessoa-card:hover {
transform: translateY(-10px) scale(1.03);
box-shadow: 0 12px 24px rgba(0,0,0,0.25);
}

.pessoa-card img {
width: 150px;
height: 150px;
border-radius: 50%;
object-fit: cover;
margin-bottom: 15px;
border: 3px solid #007bff;
}

.pessoa-card h3 {
color: #007bff;
margin-bottom: 10px;
font-size: 1.5rem;
font-weight: bold;
}

.pessoa-card p {
color: #555;
font-size: 1rem;
line-height: 1.5;
}

.pessoa-card .cargo {
font-size: 1rem;
color: #d9971c;
margin-top: -8px;
margin-bottom: 10px;
font-weight: bold;
}


/* Responsividade */
@media (max-width: 768px) {
.pessoas-wrapper {
    flex-direction: column;
    align-items: center;
}

.pessoa-card {
    width: 100%;
    max-width: 350px;
}
}

/* Animação de entrada suave */
@keyframes fadeInUp {
from {
  opacity: 0;
  transform: translateY(20px);
}
to {
  opacity: 1;
  transform: translateY(0);
}
}

.animate-fade-in {
animation: fadeInUp 0.8s ease forwards;
opacity: 0; /* Começa invisível até a animação aplicar */
}

.animate-delay {
animation-delay: 0.3s;
}




/* ========== Ajustes para Responsividade Ampla ========== */

/* Impede estouros laterais */
body, html {
  overflow-x: hidden;
}

/* Menu principal responsivo */
.menu, nav, header {
  max-width: 100%;
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  box-sizing: border-box;
}

/* Itens do menu */
.menu li {
  display: inline-block;
  padding: 10px;
}

/* Imagens e vídeos responsivos */
img, video, iframe {
  max-width: 100%;
  height: auto;
  display: block;
}

/* Media Query para smartphones */
@media (max-width: 600px) {
  .menu {
      flex-direction: column;
      align-items: center;
      text-align: center;
  }

  .menu li {
      width: 100%;
      margin-bottom: 10px;
  }

  header, nav {
      overflow-x: hidden;
  }
}
